{
    "manifest_version": 3,
    "name": "Secure Website Monitor",
    "version": "1.0",
    "action": {
      "default_popup": "templates/index.html",
      "default_icon": {
        "128": "static/logo_128.png",
        "64": "static/logo_64.png",
        "48": "static/logo_48.png",
        "32": "static/logo_32.png",
        "16": "static/logo_16.png"
      }
    },
    "permissions": ["tabs"]
  }
  
